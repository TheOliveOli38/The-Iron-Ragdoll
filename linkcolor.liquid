---
eleventyExcludeFromCollections: true
permalink: "css/linkcolor.css"
---
a[href*="bauhinia/"] {
	color: #F01ACE;
}

a[href*="fanton/"] {
	color: #B212D2;
}

a[href*="atlasia/"] {
	color: #09721A;
}

a[href*="westavia/"] {
	color: #D59528;
}

a[href*="moreland/"] {
	color: #169986;
}

a[href*="sylvandia/"],
a[href*="solarine/"],
a[href*="lunarale/"],
a[href*="mistle/"],
a[href*="holliloli/"],
a[href*="snowdale/"] {
	color: #217DE6;
}

a[href*="starcorp"] {
	color: #1A7CF0;
}
{% for ch in characters %}
a[href*="{{ ch.name | lowerCase }}/"] {
	color: {{ ch.color }};
}
{% endfor %}